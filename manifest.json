{
  "manifest_version": 3,
  "name": "Tab Time Tracker",
  "description": "Track how much time you spend on each website with advanced analytics and productivity insights.",
  "version": "2.0",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }
}
